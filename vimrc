:syntax on
:set autoindent
:set shiftwidth=4
:set tabstop=4

:set smarttab
:set expandtab

:set background=dark

:set hls
:set nu
:set ru

:filetype on            " enables filetype detection
:filetype plugin on     " enables filetype specific plugins

:set runtimepath+=~/.vim/vim-addon-manager
